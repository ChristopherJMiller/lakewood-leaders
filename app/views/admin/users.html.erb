<div class='main-div'>
  <div class="jumbotron">
    <div class="container">
      <h1 class="display-3">Users</h1>
    </div>
  </div>
  <ul class="nav nav-pills nav-justified">
    <li class="nav-item">
      <%= link_to 'Dashboard', admin_dashboard_path, class: 'nav-link' %>
    </li>
    <li class="nav-item">
      <%= link_to 'Users', admin_users_path, class: 'nav-link active' %>
    </li>
    <li class="nav-item">
      <%= link_to 'Events', admin_events_path, class: 'nav-link' %>
    </li>
  </ul>
  <table class="table table-striped table-bordered table-sm">
  <thead>
    <tr>
      <th>#</th>
      <th>Name</th>
      <th>Email</th>
      <th>Email Verified?</th>
      <th>Parents Email</th>
      <th>Parents Email Verified?</th>
      <th>Rank</th>
      <th>Creation Timestamp</th>
    </tr>
  </thead>
  <tbody>
    <%= render partial: 'user', collection: @users, as: :user %>
  </tbody>
</table>
</div>
