<div class='main-div'>
  <div class="jumbotron">
    <div class="container">
      <h1 class="display-3">Events</h1>
    </div>
  </div>
  <ul class="nav nav-pills nav-justified">
    <li class="nav-item">
      <%= link_to 'Dashboard', admin_dashboard_path, class: 'nav-link' %>
    </li>
    <li class="nav-item">
      <%= link_to 'Users', admin_users_path, class: 'nav-link' %>
    </li>
    <li class="nav-item">
      <%= link_to 'Events', admin_events_path, class: 'nav-link active' %>
    </li>
  </ul>
  <table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>#</th>
      <th>Name</th>
      <th>Description</th>
      <th>Location</th>
      <th>Start Time</th>
      <th>End Time</th>
      <th>Max Participants</th>
      <th>Participant Count</th>
      <th>Finished?</th>
      <th>Creation Timestamp</th>
    </tr>
  </thead>
  <tbody>
    <%= render partial: 'event', collection: @events, as: :event %>
  </tbody>
</table>
</div>
